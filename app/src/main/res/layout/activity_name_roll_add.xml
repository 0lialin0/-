<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include
        android:id="@+id/main_toop"
        layout="@layout/toolbar_other" />


    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/y1"
        android:background="#b2b2b2" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fadingEdge="vertical"
        android:fillViewport="false"
        android:scrollbars="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/gray2"
            android:orientation="vertical">

            <!-- 车辆信息 -->
            <LinearLayout
                android:id="@+id/car_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    style="@style/title_bg"
                    android:text="@string/green_car_info" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <RelativeLayout style="@style/electron_outer_linearLayout">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/green_car_num" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/VepPlateNo"
                                style="@style/electron_ed_content"
                                android:hint="@string/please_input" />

                        </LinearLayout>


                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_color" />

                        <RelativeLayout
                            android:id="@+id/rl_car_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/VepPlateNoColorName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_factory" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/FactoryType"
                                style="@style/electron_ed_content"
                                android:hint="@string/please_input" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_car_color" />

                        <RelativeLayout
                            android:id="@+id/rl_car_body_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:focusable="true"
                                android:focusableInTouchMode="true"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/VepColorName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />


                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_car_type" />

                        <RelativeLayout
                            android:id="@+id/rl_car_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/VehicleTypeName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />


                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView"
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_carx_type" />

                        <RelativeLayout
                            android:id="@+id/rl_veh_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/VehTypeName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <LinearLayout
                        android:id="@+id/ll_seating"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <View style="@style/view_line" />


                        <RelativeLayout
                            style="@style/electron_outer_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                style="@style/electron_tv_title"
                                android:text="@string/name_roll_seating" />

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <EditText
                                    android:id="@+id/Seating"
                                    style="@style/electron_ed_content"
                                    android:numeric="integer" />

                            </LinearLayout>

                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_zhoushu"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <View style="@style/view_line" />

                        <RelativeLayout
                            style="@style/electron_outer_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                style="@style/electron_tv_title"
                                android:text="@string/increment_zhou_shuo" />

                            <RelativeLayout
                                android:id="@+id/rl_zhoushuo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true">

                                <LinearLayout
                                    style="@style/electron_linearLayout"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/AxleCountName"
                                        style="@style/electron_ed_content" />

                                </LinearLayout>

                                <ImageView
                                    style="@style/electron_ed_image"
                                    android:src="@drawable/right_btn" />
                            </RelativeLayout>
                        </RelativeLayout>
                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/ll_dunshuo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <View style="@style/view_line" />

                        <RelativeLayout
                            style="@style/electron_outer_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                style="@style/electron_tv_title"
                                android:text="@string/increment_dun_shuo" />

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <EditText
                                    android:id="@+id/Tonnage"
                                    style="@style/electron_ed_content"
                                    android:numeric="decimal" />

                            </LinearLayout>

                        </RelativeLayout>
                    </LinearLayout>


                </LinearLayout>
            </LinearLayout>

            <!-- 违章信息-->
            <LinearLayout
                android:id="@+id/peccancy_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    style="@style/title_bg"
                    android:text="@string/name_roll_info" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/textView"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_tongxi" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/CardNo"
                                style="@style/electron_ed_content"
                                android:hint="@string/please_input" />

                        </LinearLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_weizhang" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/GenDT"
                                style="@style/electron_ed_content" />

                        </LinearLayout>

                    </RelativeLayout>


                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_weizhang_type" />

                        <RelativeLayout
                            android:id="@+id/rl_weizhang_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/PeccancyTypeName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_weizhang_addr" />

                        <RelativeLayout
                            android:id="@+id/rl_weizhang_addr"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/PeccancyOrgName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_xingshi" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/InOrgName"
                                style="@style/electron_ed_content2" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right|center_vertical"
                                android:layout_marginLeft="@dimen/x15"
                                android:layout_marginRight="@dimen/x15"
                                android:src="@drawable/bottom" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/x20"
                                android:layout_marginRight="@dimen/x30"
                                android:text="至"
                                android:textColor="@color/black2"
                                android:textSize="@dimen/x28" />

                            <TextView
                                android:id="@+id/OutOrgName"
                                style="@style/electron_ed_content2" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right|center_vertical"
                                android:layout_marginLeft="@dimen/x15"
                                android:layout_marginRight="@dimen/x15"
                                android:src="@drawable/bottom" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />


                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_height="@dimen/y200"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_qingkuang" />

                        <LinearLayout
                            android:id="@+id/ll_member_address"
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/GenCause"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/y200"
                                android:layout_gravity="left"
                                android:gravity="center_vertical"
                                android:hint="@string/please_input"
                                android:textSize="@dimen/x28" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />

                </LinearLayout>
            </LinearLayout>

            <!-- 车辆所有者信息-->
            <LinearLayout
                android:id="@+id/car_owner_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    style="@style/title_bg"
                    android:text="@string/name_roll_grey_car_all" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <RelativeLayout style="@style/electron_outer_linearLayout">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_grey_car" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/Owner"
                                style="@style/electron_ed_content"
                                android:hint="@string/please_input" />

                        </LinearLayout>


                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_grey_type" />

                        <RelativeLayout
                            android:id="@+id/rl_owner_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/OwnerTypeName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_grey_addr" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/OwnerAddress"
                                style="@style/electron_ed_content"
                                android:hint="@string/please_input" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_grey_postal" />


                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/Postalcode"
                                style="@style/electron_ed_content"
                                android:hint="@string/please_input" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />


                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_grey_phone" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/TeletePhone"
                                style="@style/electron_ed_content"
                                android:hint="@string/please_input" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />


                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_grey_mobile_phone" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/MobilePhone"
                                style="@style/electron_ed_content"
                                android:hint="@string/please_input"
                                android:numeric="integer" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />


                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_height="@dimen/y200"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/name_roll_grey_info" />


                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/HistoryInfo"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/y200"
                                android:layout_gravity="left"
                                android:gravity="center_vertical"
                                android:hint="@string/please_input"
                                android:textSize="@dimen/x28" />

                        </LinearLayout>

                    </RelativeLayout>

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll_myphotos"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:layout_marginTop="@dimen/y10"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_photo_title"
                    style="@style/title_bg"
                    android:text="@string/name_roll_photo" />

            </LinearLayout>

            <cn.wtkj.charge_inspect.views.custom.MyPhotos
                android:id="@+id/myphoto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/y20"
                android:layout_marginLeft="@dimen/x35"
                android:padding="5dp"></cn.wtkj.charge_inspect.views.custom.MyPhotos>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/y20"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/comit_button"
                    style="@style/button_layout"
                    android:background="@drawable/selector_psersoal_bt_bg"
                    android:text="提交"
                    android:textColor="@color/white" />

            </LinearLayout>


        </LinearLayout>


    </ScrollView>


</LinearLayout>
