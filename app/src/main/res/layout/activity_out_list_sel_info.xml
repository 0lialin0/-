<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include
        android:id="@+id/main_toop"
        layout="@layout/toolbar_other" />


    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/y1"
        android:background="#b2b2b2" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fadingEdge="vertical"
        android:fillViewport="false"
        android:scrollbars="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/gray2"
            android:orientation="vertical">

            <!-- 车辆信息 -->
            <LinearLayout
                android:id="@+id/car_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    style="@style/title_bg"
                    android:text="@string/outlist_info_entrance" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <RelativeLayout style="@style/electron_outer_linearLayout">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_entrance_loca" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/VepPlateNo"
                                style="@style/electron_ed_content"
                                android:hint="@string/please_input" />

                        </LinearLayout>


                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_entrance_lane" />

                        <RelativeLayout
                            android:id="@+id/rl_car_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/VepPlateNoColorName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_entrance_veh" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/FactoryType"
                                style="@style/electron_ed_content"
                                android:hint="@string/please_input" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_zhong" />

                        <RelativeLayout
                            android:id="@+id/rl_car_body_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:focusable="true"
                                android:focusableInTouchMode="true"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/VepColorName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />


                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_veh_type" />

                        <RelativeLayout
                            android:id="@+id/rl_car_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/VehicleTypeName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />


                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView"
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_chage" />

                        <RelativeLayout
                            android:id="@+id/rl_veh_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/VehTypeName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <LinearLayout
                        android:id="@+id/ll_seating"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <View style="@style/view_line" />


                        <RelativeLayout
                            style="@style/electron_outer_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                style="@style/electron_tv_title"
                                android:text="@string/outlist_info_time" />

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <EditText
                                    android:id="@+id/Seating"
                                    style="@style/electron_ed_content"
                                    android:numeric="integer" />

                            </LinearLayout>

                        </RelativeLayout>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <!-- 违章信息-->
            <LinearLayout
                android:id="@+id/peccancy_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    style="@style/title_bg"
                    android:text="@string/outlist_info_out_info" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/textView"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_out_loca" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/CardNo"
                                style="@style/electron_ed_content"
                                android:hint="@string/please_input" />

                        </LinearLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_out_lane" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/GenDT"
                                style="@style/electron_ed_content" />

                        </LinearLayout>

                    </RelativeLayout>


                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_out_veh" />

                        <RelativeLayout
                            android:id="@+id/rl_weizhang_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/PeccancyTypeName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_out_zhong" />

                        <RelativeLayout
                            android:id="@+id/rl_weizhang_addr"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true">

                            <LinearLayout
                                style="@style/electron_linearLayout"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/PeccancyOrgName"
                                    style="@style/electron_ed_content" />

                            </LinearLayout>

                            <ImageView
                                style="@style/electron_ed_image"
                                android:src="@drawable/right_btn" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_out_veh_type" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/InOrgName"
                                style="@style/electron_ed_content2" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right|center_vertical"
                                android:layout_marginLeft="@dimen/x15"
                                android:layout_marginRight="@dimen/x15"
                                android:src="@drawable/bottom" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/x20"
                                android:layout_marginRight="@dimen/x30"
                                android:text="至"
                                android:textColor="@color/black2"
                                android:textSize="@dimen/x28" />

                            <TextView
                                android:id="@+id/OutOrgName"
                                style="@style/electron_ed_content2" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right|center_vertical"
                                android:layout_marginLeft="@dimen/x15"
                                android:layout_marginRight="@dimen/x15"
                                android:src="@drawable/bottom" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />


                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_height="@dimen/y200"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_yufu_veh" />

                        <LinearLayout
                            android:id="@+id/ll_member_address"
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/GenCause"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/y200"
                                android:layout_gravity="left"
                                android:gravity="center_vertical"
                                android:hint="@string/please_input"
                                android:textSize="@dimen/x28" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_height="@dimen/y200"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_zhekou_money" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/aa"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/y200"
                                android:layout_gravity="left"
                                android:gravity="center_vertical"
                                android:hint="@string/please_input"
                                android:textSize="@dimen/x28" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />


                    <RelativeLayout
                    style="@style/electron_outer_linearLayout"
                    android:layout_height="@dimen/y200"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/electron_tv_title"
                        android:text="@string/outlist_info_yingshou_money" />

                    <LinearLayout
                        style="@style/electron_linearLayout"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/aaa"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/y200"
                            android:layout_gravity="left"
                            android:gravity="center_vertical"
                            android:hint="@string/please_input"
                            android:textSize="@dimen/x28" />

                    </LinearLayout>

                </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_height="@dimen/y200"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_shishou_money" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/aaaq"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/y200"
                                android:layout_gravity="left"
                                android:gravity="center_vertical"
                                android:hint="@string/please_input"
                                android:textSize="@dimen/x28" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_height="@dimen/y200"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_veh_num" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/aabb"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/y200"
                                android:layout_gravity="left"
                                android:gravity="center_vertical"
                                android:hint="@string/please_input"
                                android:textSize="@dimen/x28" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_height="@dimen/y200"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_licheng" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/ccc"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/y200"
                                android:layout_gravity="left"
                                android:gravity="center_vertical"
                                android:hint="@string/please_input"
                                android:textSize="@dimen/x28" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_height="@dimen/y200"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_zhoushu" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/cccb"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/y200"
                                android:layout_gravity="left"
                                android:gravity="center_vertical"
                                android:hint="@string/please_input"
                                android:textSize="@dimen/x28" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_height="@dimen/y200"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_zhongliang" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/nnn"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/y200"
                                android:layout_gravity="left"
                                android:gravity="center_vertical"
                                android:hint="@string/please_input"
                                android:textSize="@dimen/x28" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />


                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_height="@dimen/y200"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_tong_car_num" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/nnddn"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/y200"
                                android:layout_gravity="left"
                                android:gravity="center_vertical"
                                android:hint="@string/please_input"
                                android:textSize="@dimen/x28" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />

                    <RelativeLayout
                        style="@style/electron_outer_linearLayout"
                        android:layout_height="@dimen/y200"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/electron_tv_title"
                            android:text="@string/outlist_info_special_event" />

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/sdfdd"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/y200"
                                android:layout_gravity="left"
                                android:gravity="center_vertical"
                                android:hint="@string/please_input"
                                android:textSize="@dimen/x28" />

                        </LinearLayout>

                    </RelativeLayout>

                    <View style="@style/view_line" />

                </LinearLayout>
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/y20"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/comit_button"
                    style="@style/button_layout"
                    android:background="@drawable/selector_psersoal_bt_bg"
                    android:text="提交"
                    android:textColor="@color/white" />

            </LinearLayout>


        </LinearLayout>


    </ScrollView>


</LinearLayout>
