<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include
        android:id="@+id/main_toop"
        layout="@layout/toolbar_other" />


    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/y1"
        android:background="#b2b2b2" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fadingEdge="vertical"
        android:fillViewport="false"
        android:scrollbars="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/gray2"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/y14"
                android:background="@color/white"
                android:orientation="vertical">

                <RelativeLayout
                    style="@style/electron_outer_linearLayout"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/electron_tv_title"
                        android:text="@string/outlist_start_time" />

                    <LinearLayout
                        style="@style/electron_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_start_time"
                            style="@style/electron_ed_content" />

                    </LinearLayout>


                </RelativeLayout>

                <View style="@style/view_line" />

                <RelativeLayout
                    style="@style/electron_outer_linearLayout"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/electron_tv_title"
                        android:text="@string/outlist_end_time" />

                    <LinearLayout
                        style="@style/electron_linearLayout"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_end_time"
                            style="@style/electron_ed_content" />

                    </LinearLayout>


                </RelativeLayout>

                <View style="@style/view_line" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/y14"
                android:background="@color/white"
                android:orientation="vertical">

                <RelativeLayout
                    style="@style/electron_outer_linearLayout"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/electron_tv_title"
                        android:text="@string/outlist_out_loca" />

                    <RelativeLayout
                        android:id="@+id/rl_out_loca"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true">

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_out_loca"
                                style="@style/electron_ed_content" />

                        </LinearLayout>

                        <ImageView
                            style="@style/electron_ed_image"
                            android:src="@drawable/right_btn" />
                    </RelativeLayout>
                </RelativeLayout>

                <View style="@style/view_line" />

                <RelativeLayout
                    style="@style/electron_outer_linearLayout"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/electron_tv_title"
                        android:text="@string/outlist_out_veh" />

                    <RelativeLayout
                        android:id="@+id/rl_out_lane"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true">

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_out_lane"
                                style="@style/electron_ed_content" />

                        </LinearLayout>

                        <ImageView
                            style="@style/electron_ed_image"
                            android:src="@drawable/right_btn" />
                    </RelativeLayout>
                </RelativeLayout>

                <View style="@style/view_line" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/y14"
                android:background="@color/white"
                android:orientation="vertical">

                <RelativeLayout
                    style="@style/electron_outer_linearLayout"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/electron_tv_title"
                        android:text="@string/outlist_lane_type" />

                    <RelativeLayout
                        android:id="@+id/rl_lane_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true">

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_lane_type"
                                style="@style/electron_ed_content" />

                        </LinearLayout>

                        <ImageView
                            style="@style/electron_ed_image"
                            android:src="@drawable/right_btn" />
                    </RelativeLayout>
                </RelativeLayout>

                <View style="@style/view_line" />

                <RelativeLayout
                    style="@style/electron_outer_linearLayout"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/electron_tv_title"
                        android:text="@string/outlist_veh_type" />

                    <RelativeLayout
                        android:id="@+id/rl_veh_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true">

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_veh_type"
                                style="@style/electron_ed_content" />

                        </LinearLayout>

                        <ImageView
                            style="@style/electron_ed_image"
                            android:src="@drawable/right_btn" />
                    </RelativeLayout>
                </RelativeLayout>

                <View style="@style/view_line" />

                <RelativeLayout
                    style="@style/electron_outer_linearLayout"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/electron_tv_title"
                        android:text="@string/outlist_veh_seed" />

                    <RelativeLayout
                        android:id="@+id/rl_veh_seed"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true">

                        <LinearLayout
                            style="@style/electron_linearLayout"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_veh_seed"
                                style="@style/electron_ed_content" />

                        </LinearLayout>

                        <ImageView
                            style="@style/electron_ed_image"
                            android:src="@drawable/right_btn" />
                    </RelativeLayout>
                </RelativeLayout>

                <View style="@style/view_line" />

                <RelativeLayout
                    style="@style/electron_outer_linearLayout"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/electron_tv_title"
                        android:text="@string/outlist_veh_plate" />


                    <LinearLayout
                        style="@style/electron_linearLayout"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/tv_veh_plate"
                            style="@style/electron_ed_content"
                            android:hint="@string/please_input" />

                    </LinearLayout>


                </RelativeLayout>

                <View style="@style/view_line" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/y14"
                android:background="@color/white"
                android:orientation="vertical">

                <RelativeLayout
                    style="@style/electron_outer_linearLayout"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/electron_tv_title"
                        android:text="@string/outlist_car" />


                    <LinearLayout
                        style="@style/electron_linearLayout"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/tv_car_type"
                            style="@style/electron_ed_content"
                            android:hint="@string/please_input" />

                    </LinearLayout>


                </RelativeLayout>

                <View style="@style/view_line" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/y14"
                android:background="@color/white"
                android:orientation="vertical">

                <RelativeLayout
                    style="@style/electron_outer_linearLayout"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/electron_tv_title"
                        android:text="@string/outlist_charge" />


                    <LinearLayout
                        style="@style/electron_linearLayout"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/tv_charge"
                            style="@style/electron_ed_content"
                            android:hint="@string/please_input" />

                    </LinearLayout>


                </RelativeLayout>

                <View style="@style/view_line" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/y20"
                android:layout_marginTop="@dimen/y20"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/comit_button"
                    style="@style/button_layout"
                    android:background="@drawable/selector_psersoal_bt_bg"
                    android:text="查询"
                    android:textColor="@color/white" />

            </LinearLayout>


        </LinearLayout>


    </ScrollView>


</LinearLayout>
