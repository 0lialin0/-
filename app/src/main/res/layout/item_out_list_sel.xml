<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/y15"
    android:background="@drawable/corners_house_bg"
    android:clickable="false"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:id="@+id/rl_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ve_line">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/x29"
                android:layout_marginTop="@dimen/y30"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/list_sub_title"
                            android:layout_marginBottom="@dimen/y28"
                            android:text="入口车牌：" />

                        <TextView
                            android:id="@+id/active_vehPlate"
                            style="@style/list_sub_title"
                            android:layout_below="@+id/active_title"
                            android:layout_marginBottom="@dimen/y33" />
                    </LinearLayout>

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/x20"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/list_sub_title"
                        android:layout_marginBottom="@dimen/y28"
                        android:text="出口车牌：" />

                    <TextView
                        android:id="@+id/active_out_vehPlate"
                        style="@style/list_sub_title"
                        android:layout_below="@+id/active_title"
                        android:layout_marginBottom="@dimen/y33" />
                </LinearLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/list_sub_title"
                            android:layout_marginBottom="@dimen/y28"
                            android:text="入口站址：" />

                        <TextView
                            android:id="@+id/active_ent_loca"
                            style="@style/list_sub_title"
                            android:layout_below="@+id/active_title"
                            android:layout_marginBottom="@dimen/y33" />
                    </LinearLayout>


                </RelativeLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/x20"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/list_sub_title"
                        android:layout_marginBottom="@dimen/y28"
                        android:text="收费员工号：" />

                    <TextView
                        android:id="@+id/active_charge"
                        style="@style/list_sub_title"
                        android:layout_below="@+id/active_title"
                        android:layout_marginBottom="@dimen/y33"
                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/list_sub_title"
                        android:text="出口站址/车道号：" />

                    <TextView
                        android:id="@+id/active_out_loca"
                        style="@style/list_sub_title"
                        android:layout_below="@+id/active_title"
                        android:layout_marginBottom="@dimen/y33" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/list_sub_title"
                        android:text="出口时间：" />

                    <TextView
                        android:id="@+id/active_out_time"
                        style="@style/list_sub_title"
                        android:layout_below="@+id/active_title"
                        android:layout_marginBottom="@dimen/y33"
                    />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/list_sub_title"
                        android:text="车型：" />

                    <TextView
                        android:id="@+id/active_veh_type"
                        style="@style/list_sub_title"
                        android:layout_below="@+id/active_content"
                        android:layout_marginBottom="@dimen/y33"
                         />

                </LinearLayout>


            </LinearLayout>


        </RelativeLayout>

        <View
            android:id="@+id/ve_line2"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/y1"
            android:layout_below="@+id/rl_content"
            android:background="#dddddd" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ve_line2"
            android:layout_marginBottom="@dimen/y15"
            android:layout_marginTop="@dimen/y15">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:layout_marginRight="@dimen/x15"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/x15"
                    android:src="@drawable/mingdan" />

                <TextView
                    android:id="@+id/tv_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/x35"
                    android:text="@string/outlist_name" />

                <ImageView
                    android:id="@+id/iv_edit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/x15"
                    android:src="@drawable/green_add" />

                <TextView
                    android:id="@+id/tv_green"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/x35"
                    android:text="@string/outlist_green" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/x15"
                    android:src="@drawable/zhengshou" />

                <TextView
                    android:id="@+id/tv_increment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/outlist_increment" />
            </LinearLayout>


        </RelativeLayout>

    </RelativeLayout>
</LinearLayout>
